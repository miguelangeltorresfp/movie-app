{
  "name": "@awesome-movie-app/components",
  "version": "1.0.0",
  "description": "Component Library for Awesome Movie App",
  "main": "index.js",
  "repository": "https://github.com/miguelangeltorresfp/movie-app.git",
  "author": "Miguel Angel Torres FP <miguelangeltorresfp@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "type-check": "tsc --noEmit -p tsconfig.json",
    "type-check:watch": "yarn run type-check -- --watch",
    "build": "rimraf ./lib && cross-env NODE_ENV=production run-s compile:*",
    "build:watch": "rimraf ./lib && cross-env NODE_ENV=development run-p \"compile:types --watch\" \"compile:source --watch --verbose\"",
    "compile:types": "tsc --emitDeclarationOnly -p tsconfig.json",
    "compile:source": "babel src --out-dir lib --extensions \".ts,.tsx,.js,.jsx,.mjs\" --copy-files --source-maps",
    "storybook": "start-storybook -p 8080",
    "test": "jest",
    "test:staged": "jest --findRelatedTests",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "prebuild:storybook": "rimraf .jest-test-results.json && yarn test:generate-output",
    "build:storybook": "build-storybook -c .storybook -o dist/ --quiet"
  },
  "dependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.153",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "@types/react-autosuggest": "^9.3.14",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-jest": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "babel-loader": "^8.1.0",
    "bootstrap": "^4.5.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "jest": "^26.0.1",
    "jest-styled-components": "^7.0.2",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-autosuggest": "^10.0.2",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-player": "^2.1.1",
    "react-switch": "^5.0.1",
    "rimraf": "^3.0.2",
    "storybook-addon-styled-component-theme": "^1.3.0",
    "styled-components": "^5.1.0",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3"
  },
  "peerDependencies": {
    "bootstrap": "^4.5.0",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-autosuggest": "^10.0.2",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-player": "^2.1.1",
    "react-switch": "^5.0.1",
    "styled-components": "^5.1.0",
    "npm-run-all": "^4.1.5"
  }
}